{
  "expo": {
    "name": "Movie Pro",
    "slug": "Movie Pro",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.movie.moviepro"
    },
    "android": {
      "adaptiveIcon": {
        "package":"com.movie.moviepro",
        "backgroundColor": "#111111",
        "adaptiveIcon": "./assets/adaptive-icon.png"
      }
    }
  }
}
